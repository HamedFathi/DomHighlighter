{
	"compilerOptions" : {
		"module" : "none",
		"target" : "es5",
		"noImplicitAny" : false,
		"sourceMap" : true,
		"removeComments" : true,
		"emitDecoratorMetadata" : true,
		"experimentalDecorators" : true,
		"moduleResolution" : "node",
		"lib" : [
			"es2015",
			"dom"
		]
	},
	"exclude" : [
		"node_modules",
		"wwwroot",
		"typings"
	]
}
